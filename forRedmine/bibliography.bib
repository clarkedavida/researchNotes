@article{debbio__2002,
	title = {$\theta$ dependence of {SU}({N}) gauge theories},
	volume = {2002},
	issn = {1029-8479},
	url = {http://stacks.iop.org/1126-6708/2002/i=08/a=044?key=crossref.a7b0ba3e81b0a8f2a3ed33bc37cc2c8d},
	doi = {10.1088/1126-6708/2002/08/044},
	abstract = {We study the θ dependence of four-dimensional SU(N ) gauge theories, for N ≥ 3 and in the large-N limit. We use numerical simulations of the Wilson lattice formulation of gauge theories to compute the ﬁrst few terms of the expansion of the groundstate energy F (θ) around θ = 0, F (θ) − F (0) = A2 θ2(1 + b2θ2 + · · ·). Our results support Witten’s conjecture: F (θ) − F (0) = A θ2 + O(1/N ) for suﬃciently small values of θ, θ {\textless} π. Indeed we verify that the topological susceptibility has a non-zero large-N limit χ∞ = 2A with corrections of O(1/N 2), in substantial agreement with the Witten-Veneziano formula which relates χ∞ to the η mass. Furthermore, higher order terms in θ are suppressed; in particular, the O(θ4) term b2 (related to the η − η elastic scattering amplitude) turns out to be quite small: b2 = −0.023(7) for N = 3, and its absolute value decreases with increasing N , consistently with the expectation b2 = O(1/N 2).},
	language = {en},
	number = {08},
	urldate = {2018-07-16},
	journal = {J. High Energy Phys.},
	author = {Debbio, L. Del and Panagopoulos, H. and Vicari, E.},
	year = {2002},
	pages = {044},
	file = {Debbio et al. - 2002 - θ dependence of SU( iNi ) gauge theories.pdf:/home/dclarke/.zotero/zotero/xnzcwrpn.default/zotero/storage/9IB27MNS/Debbio et al. - 2002 - θ dependence of SU( iNi ) gauge theories.pdf:application/pdf}
}
